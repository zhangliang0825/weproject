    font-size: 24rpx;
    color: #82796b;
}

.card .association-entrance-box {
    background-image: -webkit-gradient(linear,left top,right top,from(#ffc572),to(#e39a51));
    background-image: -webkit-linear-gradient(left,#ffc572,#e39a51);
    background-image: -o-linear-gradient(left,#ffc572 0,#e39a51 100%);
    background-image: linear-gradient(90deg,#ffc572,#e39a51);
    border-radius: 24rpx 0 0 24rpx;
    padding: 2rpx 16rpx;
    position: absolute;
    top: 40rpx;
    right: 0;
    z-index: 11;
}

.card .association-entrance-box .icomoon {
    font-size: 28rpx;
    color: #1b1b1a;
    margin-right: 8rpx;
}

.card .association-entrance-box .entrance-value {
