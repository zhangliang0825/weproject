    font-size: 24rpx;
    color: #1b1b1a;
    letter-spacing: 0;
}

.card.active .userInfo .label .lebelExpired,.card.active .userInfo .label .lebelName,.card.active .userInfo .label .lebelName .icomoon {
    color: #e39a51;
}

.lottery-entrance {
    display: block;
    width: calc(100% - 80rpx);
    margin: 32rpx auto 0;
}

.vipDuration {
    padding: 32rpx 40rpx 0;
}

.vipDuration .vTitle {
    font-size: 24rpx;
    color: #464750;
    margin-bottom: 32rpx;
}

.vipDuration .vClass,.vipDuration .vClass .classItem {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.vipDuration .vClass {
    width: 100%;
    flex-direction: row;
    -ms-flex-pack: justify;
    justify-content: space-between;
}

.vipDuration .vClass .classItem {
    flex: 0 0 30.14%;
    background: #fff;
    border: 2rpx solid #fff;
    box-shadow: 0 0 24rpx 0 rgba(0,0,0,.1);
    padding: 40rpx 0 32rpx;
    border-radius: 8rpx;
    flex-direction: column;
    -ms-flex-pack: center;
    justify-content: center;
    position: relative;
    transition: all .2s ease-in-out;
}

.vipDuration .vClass .classItem .date,.vipDuration .vClass .classItem .price {
    line-height: 1;
    font-weight: 700;
    font-size: 28rpx;
    color: #464750;
}

.vipDuration .vClass .classItem .price {
    color: #ce9e5b;
    margin: 15rpx 0;
}

.vipDuration .vClass .classItem .price .number {
    font-weight: 700;
    font-size: 62rpx;
    color: #ce9e5b;
}

.vipDuration .vClass .classItem .oldPrice {
    line-height: 1;
    font-size: 24rpx;
    color: #b6b9c3;
    position: relative;
}

.vipDuration .vClass .classItem .oldPrice .line {
    width: 100%;
    height: 2rpx;
    background-color: #b6b9c3;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,0);
}

.vipDuration .discountMessage,.vipDuration .vClass .classItem .price-hint {
    background-image: linear-gradient(135deg,#fc8a62,#e05e5e);
}

.vipDuration .vClass .classItem .price-hint {
    line-height: 1;
    font-size: 20rpx;
    color: #fff;
    padding: 8rpx 12rpx;
    border-radius: 10rpx 0 10rpx 0;
    position: absolute;
    left: -2rpx;
    top: -20rpx;
}

.vipDuration .vClass .classItem.active {
    background: rgba(249,135,98,.06);
    border: 2rpx solid #f98762;
}

.vipDuration .vClass .classItem.active .date,.vipDuration .vClass .classItem.active .oldPrice,.vipDuration .vClass .classItem.active .price,.vipDuration .vClass .classItem.active .price .number {
    color: #f98762;
}

.vipDuration .vClass .classItem.active .oldPrice .line {
    background-color: #f98762;
}

.vipDuration .discountMessage {
    border-radius: 8rpx;
    padding: 40rpx;
    margin-top: 32rpx;
}

.vipDuration .discountMessage .row1,.vipDuration .discountMessage .row1 .option {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.vipDuration .discountMessage .row1 {
    margin-bottom: 2rpx;
    flex-direction: row;
}

.vipDuration .discountMessage .row1 .option {
    background-color: #fff;
    width: 40rpx;
    height: 40rpx;
    left: 40rpx;
    top: 36rpx;
    border-radius: 5rpx;
    color: #a5875e;
    text-align: center;
    -ms-flex-pack: center;
    justify-content: center;
    margin-right: 20rpx;
}

.vipDuration .discountMessage .row1 .option>.icomoon {
    color: #e3635f;
    font-size: 24rpx;
    margin-top: 4rpx;
}

.vipDuration .discountMessage .row1 .discountPrice {
    font-size: 32rpx;
    color: #fff;
}

.vipDuration .discountMessage .row2 {
    padding-left: 60rpx;
    opacity: .5;
    font-size: 24rpx;
    color: #fff;
}

