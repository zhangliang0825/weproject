.vip {
    background-color: #fff;
    padding-top: 40rpx;
    min-height: 100%;
    padding-bottom: 160rpx;
}

.pay-info,.vip .vip-evaluate {
    width: 100%;
}

.pay-info {
    margin-bottom: 40rpx;
}

.card,.pay-info>.price {
    height: 320rpx;
    position: relative;
}

.pay-info>.price>.img {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.pay-info .label {
    color: #a5875e;
    position: relative;
    padding-top: 190rpx;
    padding-left: 36rpx;
    display: -ms-flexbox;
    display: flex;
}

.pay-info .label>.amount {
    position: relative;
    background-image: -webkit-gradient(linear,right top,left top,from(#ffc572),to(#e39a51));
    background-image: -webkit-linear-gradient(right,#ffc572,#e39a51);
    background-image: -o-linear-gradient(right,#ffc572,#e39a51);
    background-image: linear-gradient(right,#ffc572,#e39a51);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    font-size: 76rpx;
    line-height: 1;
}

.pay-info .label>.amount>.unit {
    font-size: 48rpx;
    margin-left: 10rpx;
}

.pay-info .label>.origin {
    font-size: 32rpx;
    font-weight: 700;
    line-height: 1;
    color: #a5875e;
    position: relative;
    text-decoration: line-through;
    bottom: 0;
    padding-left: 20rpx;
    padding-top: 36rpx;
}

.pay-info>.discounts,.pay-info>.discounts>.option {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: center;
    justify-content: center;
}

.pay-info>.discounts {
    height: 140rpx;
    background-color: #a5875e;
    font-size: 32rpx;
    color: #fff;
    flex-direction: column;
    padding-left: 100rpx;
    position: relative;
}

.pay-info>.discounts>.option {
    background-color: #fff;
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    left: 40rpx;
    top: 36rpx;
    border-radius: 5rpx;
    color: #a5875e;
    text-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.pay-info>.discounts>.option>.icomoon {
    font-size: 24rpx;
    margin-top: 4rpx;
}

.pay-info>.discounts .discount-price {
    font-weight: 700;
}

.pay-info>.discounts>.expired {
    font-size: 24rpx;
    color: #d2c3ae;
}

.card {
    width: 100%;
}

.card .cardBg {
    position: absolute;
