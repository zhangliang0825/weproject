.avatar-cell {
    padding: 100rpx 60rpx;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
    position: relative;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: 50%;
}

.avatar-cell .icon-enter,.avatar-cell>.env {
    position: absolute;
    right: 30rpx;
    font-size: 30rpx;
    color: #fff;
}

.avatar-cell>.env {
    top: 10rpx;
    right: 10rpx;
    font-size: 24rpx;
}

.avatar-cell>.avatar {
    width: 120rpx;
    height: 120rpx;
    margin-right: 36rpx;
    border-radius: 50%;
    overflow: hidden;
}

.avatar-cell .cell-bd {
    color: #fff;
    display: -ms-flexbox;
    display: flex;
    flex-direction: column;
    -ms-flex-pack: center;
    justify-content: center;
}

.avatar-cell .cell-bd .username {
    font-size: 32rpx;
    margin-bottom: 10rpx;
}

.avatar-cell .cell-bd .badge {
    font-size: 20rpx;
    line-height: 32rpx;
    text-align: center;
    height: 32rpx;
    display: inline-block;
    border-radius: 4rpx;
    background-image: -webkit-gradient(linear,left top,right top,from(#dbb071),to(#c18f5d));
    background-image: -webkit-linear-gradient(left,#dbb071,#c18f5d);
    background-image: -o-linear-gradient(left,#dbb071,#c18f5d);
    background-image: linear-gradient(90deg,#dbb071,#c18f5d);
    margin-left: 8rpx;
    padding: 0 6rpx;
    vertical-align: 6rpx;
}

.avatar-cell .cell-bd .mobile {
    color: #fff;
    font-size: 24rpx;
}

.avatar-cell .cell-bd .noLogin {
    font-size: 30rpx;
    font-weight: 700;
}

.avatar-cell .cell-ft {
    position: absolute;
    right: 65rpx;
}

.avatar-cell .cell-ft:before {
    color: #fff;
    font-size: 26rpx;
}

.avatar-cell .customButton {
    padding: 0;
    border: 0;
    margin: 0;
    line-height: 1;
    background: 0 0;
}

.avatar-cell .customButton::after {
    border: 0;
}

.avatar-cell .customLogin,.com-default-cell {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.avatar-cell .customLogin {
    width: 100%;
    height: 136rpx;
    color: #fff;
}

.com-default-cell {
    background-color: #fff;
    padding: 24rpx 30rpx;
    position: relative;
}

.com-default-cell.-single {
    margin-top: 24rpx;
    margin-bottom: 48rpx;
}

.com-default-cell.-empty::before,.com-default-cell.-single::before,.com-default-cell.-speace::before {
    content: none;
}

.com-default-cell.-speace {
    margin-top: 24rpx;
}

.com-default-cell .cell-hd {
    display: -ms-flexbox;
    display: flex;
    width: 68rpx;
}

.com-default-cell .cell-hd>.icomoon {
    font-size: 45rpx;
    color: #7b7e8e;
    margin-right: 24rpx;
}

.com-default-cell .cell-hd .icon-brand-people {
    font-weight: 600;
}

.com-default-cell .cell-hd .icon-brand-people::before {
    color: #7b7e8e;
}

.com-default-cell .cell-hd>.cover {
    display: block;
    width: 64rpx;
    height: 64rpx;
    margin-right: 24rpx;
    background-color: #fff;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: 50%;
}

.com-default-cell .cell-hd>.logo {
    display: block;
    width: 44rpx;
    height: auto;
}

.com-default-cell .cell-bd {
    flex: 1;
}

.com-default-cell .cell-bd>.name {
    font-size: 28rpx;
    font-weight: 400;
    color: #464750;
    display: block;
}

.com-default-cell .cell-bd>.datetime {
    font-size: 20rpx;
    color: #7b7e8e;
    display: block;
}

.com-default-cell .cell-bd>.text {
    color: #7b7e8e;
    font-size: 28rpx;
    text-align: left;
    padding: 0;
    background-color: #fff;
}

.com-default-cell .cell-bd>.text:after {
    display: none;
}

.com-default-cell .cell-ft {
    position: absolute;
    right: 30rpx;
}

.com-default-cell .cell-ft:before {
    color: #c7c7cc;
    font-size: 26rpx;
}

.com-default-cell .new {
    font-weight: 700;
    font-size: 22rpx;
    color: #fff;
    letter-spacing: 0;
    padding: 4rpx 10rpx;
    background: #ec586a;
    border-radius: 4rpx;
    position: absolute;
    right: 78rpx;
}

.com-register-succ .register-box {
    width: calc(100% - 80rpx);
    background: #fff;
    border-radius: 8rpx;
    overflow: hidden;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 999;
}

.com-register-succ .register-box .registerTitle {
    display: -ms-flexbox;
    display: flex;
    flex-direction: row;
    -ms-flex-align: center;
    align-items: center;
    margin: 39rpx 52rpx 0;
    font-size: 48rpx;
    color: #464750;
    font-weight: 700;
}

.com-register-succ .register-box .registerTitle .line {
    width: 6rpx;
    height: 48rpx;
    background: #c39d63;
    margin-right: 15rpx;
}

.com-register-succ .register-box .registerDescribe {
    font-size: 28rpx;
    color: #7b7e8e;
    line-height: 1.5;
    margin: 39rpx 52rpx;
}

.com-register-succ .register-box .registerButtons,.com-register-succ .register-box .registerButtons .dialogButton {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.com-register-succ .register-box .registerButtons {
    border-top: 1rpx solid #e3e3e3;
    flex-direction: row;
}

.com-register-succ .register-box .registerButtons .dialogButton {
    width: 100%;
    font-size: 28rpx;
    color: #7b7e8e;
    -ms-flex-pack: center;
    justify-content: center;
    padding: 23rpx 0 24rpx;
}

.dashboard {
    background-color: #fff;
    padding-bottom: 88rpx;
}

.vip {
    padding: 28rpx 20rpx 0;
}

.vipEntry {
    position: relative;
    height: 160rpx;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.vipImage {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.vipLabel {
    position: relative;
    color: #e9c188;
    font-size: 32rpx;
    line-height: 1;
    padding-left: 132rpx;
}

.vipLabel>.expired {
    font-size: 20rpx;
    line-height: 20rpx;
    color: #f2d0a0;
    margin-top: 10rpx;
}

.cells {
    margin-top: 20rpx;
}